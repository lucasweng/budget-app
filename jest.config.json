{
  "setupFiles": [
    "raf/polyfill",
    "<rootDir>/src/__tests__/setupTests.js"
  ],
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "testPathIgnorePatterns": [
    "<rootDir>/src/__tests__/fixtures/",
    "<rootDir>/src/__tests__/__mocks__/",
    "<rootDir>/src/__tests__/setupTests.js"
  ],
  "coverageDirectory": "./coverage/",
  "collectCoverage": true
}
